{
    "name": "EDsnap",
    "description": "Hackathon stuff",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "images/snap-icon-16.png",
      "32": "images/snap-icon-32.png",
      "48": "images/snap-icon-48.png",
      "128": "images/snap-icon-128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*.enable.com/*"],
        "js": ["script.js", "snapPanel.js", "rules/ruleParser.js", "constants.js", "main.js", "rules/ruleDictionary.js", "rules/globalRuleParser.js", "rules/globalRuleDictionary.js", "edsParser.js"],
        "css": ["styles.css"],
        "run_at": "document_idle"
      }
    ],
    "permissions": [
    "activeTab",
    "scripting"
  ],
    "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/EDSnap_Logo.png", "styles.css", "images/logo-green.png", "images/logo-black.png"],
      "matches":["https://*.enable.com/*"]
    }
  ]
  }