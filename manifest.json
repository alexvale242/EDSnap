{
    "name": "EDsnap",
    "description": "Hackathon stuff",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "hello.html"
    },
    "icons": {
      "16": "snap-icon-16.png",
      "32": "snap-icon-32.png",
      "48": "snap-icon-48.png",
      "128": "snap-icon-128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*.enable.com/*"],
        "js": ["script.js", "snapPanel.js", "ruleParser.js", "constants.js", "main.js", "ruleDictionary.js", "globalRuleParser.js", "globalRuleDictionary.js", "edsParser.js"],
        "css": ["styles.css"],
        "run_at": "document_idle"
      }
    ],
    "permissions": [
    "activeTab",
    "scripting"
  ],
    "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["EDSnap_Logo.png"],
      "matches":["https://*.enable.com/*"]
    }
  ]
  }