{
    "name": "EDsnap",
    "description": "Hackathon stuff",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "hello.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*.enable.com/*"],
        "js": ["script.js", "script_lc.js"],
        "css": ["styles.css"],
        "run_at": "document_idle"
      }
    ],
    "permissions": [
    "activeTab",
    "scripting"
  ],
    "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
  }